    {

        "version": 2,

        "builds": [

            {

                "src": "main.py",

                "use": "@vercel/node",

                "command": "uvicorn main:app --host 0.0.0.0 --port $PORT"

            }

        ],

        "routes": [

            {

                "src": "/(.*)",

                "dest": "/api/{{1}}"

            }

        ],

        "env": {

            "PORT": "80"

        }

    }
